{
  "name": "catter-server",
  "version": "0.0.1",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "log:clear": "rm -rf logs",
    "prisma:reset": "sudo prisma reset --force",
    "prisma:deploy": "sudo prisma deploy --force",
    "seed": "ts-node ./script/seed.ts",
    "start": "ts-node index.ts",
    "dev": "ts-node-dev --no-notify --respawn  ./",
    "dev:inspect": "ts-node-dev --inspect --no-notify --respawn  ./",
    "build": "tsc",
    "lint": "eslint '**/*.ts'",
    "nexus-prisma-generate": "npx nexus-prisma-generate --client ./generated/prisma-client --output ./generated/nexus-prisma"
  },
  "dependencies": {
    "@types/zxcvbn": "^4.4.0",
    "apollo-server": "^2.8.1",
    "argon2": "^0.24.0",
    "fp-ts": "^2.0.0",
    "graphql": "^14.3.1",
    "graphql-middleware": "^4.0.0",
    "graphql-shield": "^6.0.6",
    "graphql-yoga": "^1.17.4",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^5.0.0",
    "module-alias": "^2.2.1",
    "nexus": "^0.11.7",
    "nexus-prisma": "^0.3.7",
    "prisma-client-lib": "^1.34.0",
    "ramda": "^0.26.1",
    "the-big-username-blacklist": "^1.5.2",
    "yup": "^0.27.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@types/graphql": "14.2.3",
    "@types/jsonwebtoken": "8.3.2",
    "@types/ramda": "types/npm-ramda#dist",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "eslint": "6.0.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-unicorn": "10.0.0",
    "nexus-prisma-generate": "0.3.7",
    "ts-node": "8.3.0",
    "ts-node-dev": "1.0.0-pre.40",
    "typescript": "3.4.5"
  },
  "resolutions": {
    "graphql": "^14.3.1"
  },
  "_moduleAliases": {
    "@/common": "src/common",
    "@/utils": "src/utils",
    "@/constants": "src/constants",
    "@/resolvers": "src/resolvers",
    "@/middlewares": "src/middlewares"
  }
}
