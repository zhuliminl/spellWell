type User {
  id: ID! @id
  email: String @unique
  name: String!
  job: String
  nickname: String
  username: String! @default(value: "saul") 
  age: String
  posts: [Post!]!
}

type Post {
  id: ID! @id
  title: String!
  published: Boolean! @default(value: false)
  author: User @relation(link: INLINE)
}